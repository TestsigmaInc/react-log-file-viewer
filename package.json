{
    "name": "@testsigmainc/react-log-file-viewer",
    "version": "1.1.0",
    "description": "A React component for viewing large text files.",
    "main": "dist/index.js",
    "module": "dist/index.js",
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "require": "./dist/index.js"
        }
    },
    "publishConfig": {
        "registry": "https://npm.pkg.github.com"
    },
    "files": [
        "dist",
        "src"
