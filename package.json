    "scripts": {
        "build": "babel src --out-dir dist --copy-files",
        "prepublishOnly": "npm run build"
    },
    "keywords": [
        "react",
        "component",
        "text file",
        "viewer",
        "large file",
        "log viewer",
        "file viewer"
