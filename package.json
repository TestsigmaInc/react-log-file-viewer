{
    "name": "react-log-file-viewer",
    "version": "1.1.0",
    "description": "A React component for viewing large text files.",
    "main": "dist/index.js",
    "module": "src/index.js",
    "exports": {
        ".": {
            "import": "./src/index.js",
            "require": "./dist/index.js"
        }
    },
    "scripts": {
        "build": "babel src --out-dir dist --copy-files",
        "prepublish": "npm run build"
    },
    "keywords": [
        "react",
        "component",
        "text file",
        "viewer",
        "large file"
    ],
    "author": "Vinodh Kumar G",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/TestsigmaInc/react-log-file-viewer"
    },
    "peerDependencies": {
        "react": "^18.2.0",
        "react-dom": "^18.2.0"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ],
    "devDependencies": {
        "@babel/cli": "^7.23.4",
        "@babel/preset-react": "^7.22.5"
    }
}
