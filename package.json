    "author": "Vinodh Kumar G",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/TestsigmaInc/react-log-file-viewer.git"
    },
    "bugs": {
        "url": "https://github.com/TestsigmaInc/react-log-file-viewer/issues"
    },
    "homepage": "https://github.com/TestsigmaInc/react-log-file-viewer#readme",
    "engines": {
        "node": ">=16.0.0"
    },
    "peerDependencies": {
        "react": "^18.2.0",
        "react-dom": "^18.2.0"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ],
    "devDependencies": {
        "@babel/cli": "^7.23.4",
        "@babel/preset-react": "^7.22.5"
